	<WorkGiverDef>
		<defName>QE_OrderProcessor_OrganVat</defName>
		<label>process order for organ vat</label>
		<giverClass>QEthics.WorkGiver_GrowerOrderProcessor</giverClass>
		<workType>Hauling</workType>
		<verb>fill</verb>
