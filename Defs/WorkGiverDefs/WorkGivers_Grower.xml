			<li>Manipulation</li>
		</requiredCapacities>
		<fixedBillGiverDefs>
			<li>QE_OrganVat</li>
		</fixedBillGiverDefs>
	</WorkGiverDef>

	<WorkGiverDef Name="OrganVatMaintainBase" Abstract="True">
		<label>maintains a organ vat</label>
		<giverClass>QEthics.WorkGiver_MaintainGrower</giverClass>
		<verb>maintain</verb>
		<gerund>maintaining</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
			<li>Sight</li>
		</requiredCapacities>
		<fixedBillGiverDefs>
			<li>QE_OrganVat</li>
		</fixedBillGiverDefs>
	</WorkGiverDef>

	<WorkGiverDef ParentName="OrganVatMaintainBase">
		<defName>QE_OrganGrower_Maintain</defName>
		<workType>QE_MaintainVat</workType>
		<priorityInType>1</priorityInType>
	</WorkGiverDef>

