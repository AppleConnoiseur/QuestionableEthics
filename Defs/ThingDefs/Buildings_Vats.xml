		<costList>
			<Steel>325</Steel>
			<Silver>225</Silver>
			<Plasteel>75</Plasteel>
			<ComponentSpacer>3</ComponentSpacer>
		</costList>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<researchPrerequisites>
			<li>QE_VatGrownBeings</li>
		</researchPrerequisites>
		<statBases>
			<MaxHitPoints>1000</MaxHitPoints>
			<WorkToBuild>10000</WorkToBuild>
			<Flammability>0.5</Flammability>
			<Beauty>5</Beauty>
			<Mass>145</Mass>
			<Cleanliness>2.4</Cleanliness>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>700</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>2.75</glowRadius>
				<glowColor>(181,230,29,0)</glowColor>
			</li>
		</comps>
		<modExtensions>
			<li Class="QEthics.VatGrowerProperties">
				<bottomGraphic>
					<graphicClass>Graphic_Single</graphicClass>
					<texPath>Things/Building/QEVatBottom</texPath>
					<drawSize>(6,6)</drawSize>
					<drawRotated>false</drawRotated>
				</bottomGraphic>
				<topGraphic>
					<graphicClass>Graphic_Single</graphicClass>
					<texPath>Things/Building/QEVatTop</texPath>
					<drawSize>(6,6)</drawSize>
					<drawRotated>false</drawRotated>
				</topGraphic>
				<glowGraphic>
					<graphicClass>Graphic_Single</graphicClass>
					<texPath>Things/Building/QEVatGlow</texPath>
					<shaderType>MoteGlow</shaderType>
					<drawSize>(6,6)</drawSize>
					<drawRotated>false</drawRotated>
				</glowGraphic>
				<topDetailGraphic>
					<graphicClass>Graphic_Single</graphicClass>
					<texPath>Things/Building/QEVatTopDetail</texPath>
					<shaderType>MoteGlow</shaderType>
					<drawSize>(6,6)</drawSize>
					<drawRotated>false</drawRotated>
				</topDetailGraphic>
			</li>
		</modExtensions>
	</ThingDef>
</Defs>