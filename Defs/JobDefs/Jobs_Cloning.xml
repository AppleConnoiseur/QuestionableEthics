		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
		<modExtensions>
			<li Class="QEthics.RecipeOutcomeProperties">
				<outputThingDef>QE_GenomeSequencerFilled</outputThingDef>
			</li>
		</modExtensions>
